          "name": "README.md",
          "type": "markdown",
          "template": "documentation"
        },
        {
          "name": "index.html",
          "type": "html",
          "template": "modern"
        },
        {
          "name": "session-data.json",
          "type": "json",
          "template": "raw"
        },
        {
          "name": "screenshots",
          "type": "directory",
          "content": "screenshots"
        },
        {
          "name": "assets",
          "type": "directory",
          "content": ["css", "js", "images"]
        }
      ]
    }
  },
