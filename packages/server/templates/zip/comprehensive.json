{
  "id": "comprehensive",
  "name": "Comprehensive ZIP Package",
  "description": "Complete export package with all formats and assets",
  "format": "zip",
  "type": "built-in",
  "config": {
    "styles": "",
    "header": "",
    "footer": "",
    "stepTemplate": "",
    "layout": {
      "pageSize": "A4",
      "orientation": "portrait",
      "margins": {
        "top": 0,
        "right": 0,
        "bottom": 0,
        "left": 0
      }
    },
    "colors": {
      "primary": "#2563eb",
      "secondary": "#64748b",
      "accent": "#f59e0b",
      "background": "#ffffff",
      "text": "#1e293b"
    },
    "includeFormats": ["markdown", "html", "json"],
    "includeRawScreenshots": true,
    "includeSessionData": true,
